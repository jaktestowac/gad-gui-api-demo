{
  "name": "gad-gui-api-demo-v2",
  "version": "2.8.4",
  "description": "Simple application with GUI and API to deploy to Heroku, Glitch, Railway, Vercel etc.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start3001": "set PORT=3001 && node server.js",
    "p:start": "pm2 start ecosystem.config.js",
    "p:list": "pm2 list",
    "p:stop": "pm2 stop gad2",
    "p:del": "pm2 delete gad2",
    "p:log": "pm2 logs gad2",
    "p:kill": "pm2 kill",
    "pretty": "npx prettier --write .",
    "eslint": "npx eslint .",
    "start:readonly": "set READ_ONLY=1 && node server.js",
    "start:ssl": "set SSL_ENABLED=1 && node server.js"
  },
  "keywords": [
    "json-server, node, REST API"
  ],
  "author": "jaktestowac.pl",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-pause": "^0.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.4",
    "ejs": "^3.1.8",
    "errorhandler": "^1.2.0",
    "express": "^4.16.2",
    "express-urlrewrite": "^1.2.0",
    "formidable": "^3.5.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.11.2",
    "lodash-id": "^0.14.0",
    "lowdb": "^0.15.0",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.9",
    "otpauth": "^9.4.0",
    "pluralize": "^7.0.0",
    "prettier": "2.8.4",
    "qrcode": "^1.5.4",
    "seedrandom": "^3.0.5",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-plugin-html": "^7.1.0"
  }
}
