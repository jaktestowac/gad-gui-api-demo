# Test Server Configuration
# Copy this file to .env and modify the values as needed

# Server host (default: localhost)
TEST_SERVER_HOST=localhost

# Server port (default: 3002)
TEST_SERVER_PORT=3002

# Base URL (optional - will be auto-generated from host and port if not provided)
# TEST_SERVER_BASE_URL=http://localhost:3002

# Server start timeout in milliseconds (default: 30000)
TEST_SERVER_START_TIMEOUT=30000

# Enable debug mode (default: false)
TEST_SERVER_DEBUG=false 